{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": ["airbnb-base", "plugin:prettier/recommended", "import"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "prettier"],
	"rules": {
		"no-console": "warn"
	}
}
